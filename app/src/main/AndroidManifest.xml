<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mosh.bo.doctorapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DoctorApp">
        <activity
            android:name=".doctor.DoctorActivity"
            android:label="@string/title_activity_doctor2"></activity>
        <activity android:name=".patient.PatientActivity" />
        <activity
            android:name=".login.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.DoctorApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>